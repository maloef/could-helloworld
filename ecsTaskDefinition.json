{
  "family": "capstoneTaskDefinitionFamily",
  "taskRoleArn": "arn:aws:iam::818222958857:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::818222958857:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requires_compatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "HelloWorld",
      "image": "818222958857.dkr.ecr.eu-central-1.amazonaws.com/capstone-repository",
      "repositoryCredentials": {
        "credentialsParameter": ""
      },
      "cpu": 256,
      "memory": 512,
      "memoryReservation": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
